
void
@test_proc 
unary_expressions() {
    int x = 42;
    int y = -x;
    assert(y == -42);
    
    bool flag = true;
    flag = !flag;
    assert(flag == false);
}

void
@test_proc
basic_referencing() {
    int x = 10;
    int* y = &x;
    int* z = &x;
    assert(y == z);
}

void 
@test_proc
if_statements() {
    int x = 10;
    if (x == 10) {
        assert(1);
    } else {
        assert(0);
    }
}
